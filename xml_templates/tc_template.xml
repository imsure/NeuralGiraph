<?xml version="1.0" encoding="UTF-8"?>

<!-- A partition of the neural network, the partition contains "tc" neurons -->
<partition type="tc">
  <!-- Description of the neurons inside the partition -->
  <neuron>
    <!-- To be filled by Python parser -->
    <start_id>xxx</start_id>
    <!-- To be filled by Python parser -->
    <end_id>yyy</end_id>
    
    <potential>65</potential>
    
    <parameter name="a">
      <!-- a = factor1 + factor2 * random_number(0~1) -->
      <factor1>0.002</factor1>
      <factor2>0</factor2>
    </parameter>

    <parameter name="b">
      <!-- b = factor1 - factor2 * random_number(0~1) -->
      <factor1>0.25</factor1>
      <factor2>0</factor2>
    </parameter>

    <parameter name="c">
      <!-- c = factor1 + factor2 * random_number(0~1)^2 -->
      <factor1>-65</factor1>
      <factor2>15</factor2>
    </parameter>

    <parameter name="d">
      <!-- d = factor1 - factor2 * random_number(0~1)^2 -->
      <factor1>0</factor1>
      <factor2>-0.05</factor2>
    </parameter>

  </neuron>

</partition>
