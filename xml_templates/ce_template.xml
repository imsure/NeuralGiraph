<?xml version="1.0" encoding="UTF-8"?>

<!-- A partition of the neural network, the partition contains "ce" neurons -->
<partition type="ce">
  <!-- Description of the neurons inside the partition -->
  <neuron>
    <!-- To be filled by Python parser -->
    <start_id>xxx</start_id>
    <!-- To be filled by Python parser -->
    <end_id>yyy</end_id>
    
    <potential>65</potential>
    
    <parameter name="a">
      <!-- a = factor1 + factor2 * random_number(0~1) -->
      <factor1>0.02</factor1>
      <factor2>0</factor2>
    </parameter>

    <parameter name="b">
      <!-- b = factor1 - factor2 * random_number(0~1) -->
      <factor1>0.2</factor1>
      <factor2>0</factor2>
    </parameter>

    <parameter name="c">
      <!-- c = factor1 + factor2 * random_number(0~1)^2 -->
      <factor1>-65</factor1>
      <factor2>15</factor2>
    </parameter>

    <parameter name="d">
      <!-- d = factor1 - factor2 * random_number(0~1)^2 -->
      <factor1>8</factor1>
      <factor2>6</factor2>
    </parameter>

  </neuron>

  <connection>
    <!-- Describes types of neurons "ce" connects to, it is outgoing connection -->
    <to type="ce">
      <probability>1</probability>
      <strength>0.5</strength>
    </to>

    <to type="ci">
      <probability>1</probability>
      <strength>0.5</strength>
    </to>

    <to type="stn">
      <probability>0.25</probability>
      <strength>0.25</strength>
    </to>

    <to type="strd1">
      <probability>0.5</probability>
      <strength>0.2</strength>
    </to>

    <to type="strd2">
      <probability>0.5</probability>
      <strength>0.2</strength>
    </to>
  </connection>
</partition>
